#!/command/execlineb -P

exec s6-setuidgid ${PUID}:${PGID} /usr/bin/rclone rcd \
    --config=/config \
    --rc-web-gui \
    --rc-addr=0.0.0.0:5575 \
    --rc-web-gui-no-open-browser \
    --rc-no-auth \
    --transfers=1 \
    --bwlimit=10M \
    --multi-thread-streams=1 \
    /var/lib/rclonewebui